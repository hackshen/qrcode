{
    "name": "Hshen",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "qrCode",
    "browser_action": {
        "default_title": "Title",
        "default_icon": "favicon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["*://*/*"]
        }
    ],
    "permissions" : ["tabs", "activeTab"] //向浏览器申请的权限
}
