{
    "name": "Hshen",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "qrCode",
    "browser_action": {
        "default_title": "Title",
        "default_icon": "favicon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "inject.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "commands": {
        "_execute_browser_action": {
            "suggested_key": {
                "default": "Alt+Shift+S",
                "mac": "Command+Shift+S"
            }
        }
    },
    "permissions": [
        "tabs",
        "activeTab",
        "contextMenus",
        "declarativeContent",
        "downloads",
        "clipboardRead",
        "clipboardWrite",
        "chrome://net-internals/#events",
        "chrome://extensions/"
    ]
    //向浏览器申请的权限
}
